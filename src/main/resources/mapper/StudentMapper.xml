<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.dataoperator.dao.StudentDao">

    <insert id="save" parameterType="com.example.dataoperator.dto.StudentVo">
        INSERT INTO `zz_my`.`student`(
        `name`,
        `score`
        )
        VALUES (
        #{name},
        #{score}
        );

    </insert>

</mapper>